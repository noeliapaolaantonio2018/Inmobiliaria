@model Inmobiliaria.Models.Inquilinos

@{ ViewData["Title"] = "Eliminar Inquilino"; }

<p>
    <h2>Eliminar Inquilino</h2>

    <h6 class="alert alert-warning"> Usted esta seguro que desea Eliminar el Inquilino???</h6>
</p>
<div>

    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <label>Codigo Inquilino</label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdInq)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Apellido)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Apellido)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Dni)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Dni)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Telefono)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Telefono)
        </dd>
        <dt class="col-sm-2">
            <label>Direccion Trabajo</label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DireccionTrabajo)
        </dd>
        <dt class="col-sm-2">
            <label>Nombre Garante</label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nombre_Garante)
        </dd>
        <dt class="col-sm-2">
            <label>Dni Garante</label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Dni_Garante)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="submit" value="Borrar" class="btn btn-danger" /> |
        <a class="btn btn-info" asp-action="Index">Regresar Listado</a>
    </form>
</div>

